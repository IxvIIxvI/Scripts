

--Ninja Legends X


--[[This Script was Made by Ixvi https://www.youtube.com/channel/UCtV09HtIpyhnrg6uNFC5Avw
    Also Credits To whoever made Kavo Ui]]--





local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ninja Legends X", "Ocean")



--Tabs
local MainTab = Window:NewTab("Main")
local VisualsTab = Window:NewTab("Visuals")
local LocalPlayerTab = Window:NewTab("LocalPlayer")
local OtherTab = Window:NewTab("Other")
local TeleportTab = Window:NewTab("Teleport")
local ChestsTab = Window:NewTab("Chest")
local GuiTab = Window:NewTab("Gui")






--Sections
local MainSection = MainTab:NewSection("Main")
local VisualSection = VisualsTab:NewSection("Visuals")
local LocalPlayerSection = LocalPlayerTab:NewSection("LocalPlayer")
local GuiSection = GuiTab:NewSection("Gui")
local OtherSection = OtherTab:NewSection("Other")
local TeleportSection = TeleportTab:NewSection("Teleport")
local ChestSection = ChestsTab:NewSection("Chest")


















--MainSection Toggles

MainSection:NewToggle("AutoSwing", "AutoSwingsForYou", function(state)
    if state then
        getgenv().AutoKatana = true;
        while wait() do
            if getgenv().AutoKatana == true then
                game:GetService("Players").LocalPlayer.ninjaEvent:FireServer("swingKatana") -- Script generated by TurtleSpy, made by Intrer#0421
            end
        end
    else
        getgenv().AutoKatana = false;
    end
end)

MainSection:NewToggle("CoinHoopFarm", "Delay of like 7 sec  seconds", function(state)
    if state then
        getgenv().CoinFarm = true;
        while wait() do
            if getgenv().CoinFarm == true then
                game:GetService("ReplicatedStorage").rEvents.hoopEvent:FireServer("useHoop",workspace.Hoops.Hoop) -- Script generated by TurtleSpy, made by Intrer#0421
            end
        end
    else
        getgenv().CoinFarm = false;
    end
end)

MainSection:NewButton("GroupRewards", "Give You Group Rewards", function()
    game:GetService("ReplicatedStorage").rEvents.groupRemote:InvokeServer("groupRewards") -- Script generated by TurtleSpy, made by Intrer#0421
end)

MainSection:NewButton("OpenWheel", "Opens wheel", function()
    game:GetService("ReplicatedStorage").rEvents.openFortuneWheelRemote:InvokeServer("openFortuneWheel",workspace["Fortune Wheel"]) -- Script generated by TurtleSpy, made by Intrer#0421
end)


MainSection:NewButton("Sell", "Sells Sutff", function()
    local TS = game:GetService("TweenService")
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1), {CFrame = CFrame.new(85.5726471, 2.9980247, -54.8876915)})
    tween:play()
end)



-- Gets Cframe setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame))


--Visuals






VisualSection:NewToggle("FunnyCam", "Changes Camera Type", function(state)
    if state then
        game.Workspace.Camera.CameraType = "Watch"
    else
        game.Workspace.Camera.CameraType = "Custom"
    end
end)







VisualSection:NewToggle("Chat Changer", "Changes Possition of chat", function(state)
    if state then
        game:GetService("StarterGui"):SetCore('ChatWindowPosition', UDim2.new(0, 1380, 0.0, 700))
    else
        game:GetService("StarterGui"):SetCore('ChatWindowPosition', UDim2.new(0.0, 0, 0.0, 0))
    end
end)



VisualSection:NewToggle("Cloud Trail", "Creates A Trail of Clouds", function(state)
    if state then
        getgenv().Trail = true;
        while wait(1) do
            if getgenv().Trail == true then
                game:GetService("Players").LocalPlayer.ninjaEvent:FireServer("multiJump","rightLeg") -- Script generated by TurtleSpy, made by Intrer#0421
            end
        end
    else
        getgenv().Trail = false;
    end
end)



VisualSection:NewSlider("FovChanger", "Changes Field Of View", 120, 70, function(t) -- 500 (MaxValue) | 0 (MinValue)
    game.Workspace.Camera.FieldOfView = t
end)






--Player Tab








LocalPlayerSection:NewToggle("Bhop", "yes", function(state)
    if state then
        getgenv().AutoJump = true;
        while wait(2) do
            if getgenv().AutoJump == true then
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
            end
        end
    else
        getgenv().AutoJump = false;
    end
end)







LocalPlayerSection:NewSlider("Speed", "Changes Walkspeed", 300, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

LocalPlayerSection:NewSlider("Gravity", "Changes Gravity", 1, 196.2, function(r) -- 500 (MaxValue) | 0 (MinValue)
    game.Workspace.Gravity = r
end)

LocalPlayerSection:NewSlider("JumpPower", "Changes JumpPower", 600, 50, function(e) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = e
end)

LocalPlayerSection:NewButton("GodMode", "Sadly You Can Not Kill Anyone", function()
    local GodMode = game.Players.LocalPlayer.Character
    GodMode.Parent = nil
    GodMode.HumanoidRootPart:Destroy()
    GodMode.Parent = game.workspace
end)




--other

OtherSection:NewTextBox("ChatMessage", "Sends a chat message from here", function(txt)
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(txt,"All") -- Script generated by TurtleSpy, made by Intrer#0421(txt) 
end)


OtherSection:NewToggle("Funny", "yes", function(state)
    if state then
        game.Workspace.Gravity = 0
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 600
        game.Players.LocalPlayer.Character.Humanoid.Jump = true
    else
        game.Workspace.Gravity = 192.6
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)


local DiscordSection = OtherTab:NewSection("It Will Copy A Link To Your clipboard ⬇️")

DiscordSection:NewButton("Discord", "Sends a link to your clipboard", function()
    setclipboard("https://discord.com/invite/ixvi")
end)


--Teleport


TeleportSection:NewButton("HillKing", "Goes To The Hill", function()
    local TS = game:GetService("TweenService")
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1), {CFrame = CFrame.new(227.762482, 89.7980728, -272.215759)})
    tween:play()
end)

TeleportSection:NewButton("Spawn", "Goes To Spawn", function()
    local TS = game:GetService("TweenService")
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1), {CFrame = CFrame.new(4.56986856, 3.19602442, 48.3547935)})
    tween:play()
end)

TeleportSection:NewButton("Astral Island", "Goes To Astral Island", function()
    local TS = game:GetService("TweenService")
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1), {CFrame = CFrame.new(127.635674, 2013.65356, 245.499512)})
    tween:play()
end)

TeleportSection:NewButton("Enchanted Island", "Goes To Enchanted Island", function()
    local TS = game:GetService("TweenService")
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1), {CFrame = CFrame.new(28.8792133, 766.237427, -113.869362)})
    tween:play()
end)


