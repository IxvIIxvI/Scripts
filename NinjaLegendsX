    local TS = game:GetService("TweenService")
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1), {CFrame = CFrame.new(181.852188, 5656.6543, 145.560898)})
    tween:play()
end)

