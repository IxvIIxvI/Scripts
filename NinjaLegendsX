-- Gets Cframe setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame))







--Chests

ChestSection:NewButton("Magma Chest", "Collects Magma Chest", function()
    game:GetService("ReplicatedStorage").rEvents.checkChestRemote:InvokeServer("Magma Chest") -- Script generated by TurtleSpy, made by Intrer#0421
end)


ChestSection:NewButton("Mythical Chest", "Collects Mythical Chest", function()
    game:GetService("ReplicatedStorage").rEvents.checkChestRemote:InvokeServer("Mythical Chest") -- Script generated by TurtleSpy, made by Intrer#0421
end)

ChestSection:NewButton("Lengends Chest", "Collects Lengends Chest", function()
    game:GetService("ReplicatedStorage").rEvents.checkChestRemote:InvokeServer("Legends Chest") -- Script generated by TurtleSpy, made by Intrer#0421
end)



--Gui


GuiSection:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.G, function()
	Library:ToggleUI()
end)










